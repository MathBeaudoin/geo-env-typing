{
    "name": "geo-env-typing",
    "version": "1.1.1",
    "description": "A package aimed to provide types, classes and data-validators for geographic, solar, and air-quality-data. Currently supporting TypeScript ",
    "keywords": [
        "solar",
        "air quality",
        "gis",
        "types"
    ],
    "type": "module",
    "author": "MathBeaudoin",
    "repository": {
        "type": "git",
        "url": "https://github.com/MathBeaudoin/geo-env-typing.git"
    },
    "scripts": {
        "build": "npm run test && npm run prettier && npm run clean && npm run build:esm && npm run build:cjs && npm run copy-package-file",
        "test": "vitest run",
        "prettier": "prettier --trailing-comma none --tab-width 4 --print-width 120 -uw --cache --ignore-path .gitignore .",
        "clean": "if exist .\\dist rmdir /S /Q .\\dist",
        "build:esm": "tsc -p .\\tsconfig.esm.json",
        "build:cjs": "tsc -p .\\tsconfig.cjs.json",
        "copy-package-file": "copy .\\package.json .\\dist\\package.json",
        "prepack": "npm run build"
    },
    "devDependencies": {
        "prettier": "^3.2.5",
        "tsx": "^4.7.1",
        "typescript": "^5.3.3",
        "vitest": "^1.3.0"
    },
    "dependencies": {
        "@faker-js/faker": "^8.4.1"
    }
}
